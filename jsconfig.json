{
  "compilerOptions": {
    "baseUrl": "./server",
    "paths": {
      "#data/*": ["data/*"],
      "#config/*": ["config/*"],
      "#models/*": ["models/*"],
      "#controllers/*": ["controllers/*"],
      "#routes/*": ["routes/*"],
      "#middlewares/*": ["middlewares/*"],
      "#utils/*": ["utils/*"]
    }
  },
  "include": ["server/**/*"],
  "exclude": ["node_modules"]
}
